<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.itparadise.jenjudan">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".user.activity.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".user.activity.P_Payment_Success"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".user.activity.P_Payment_History"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".user.activity.P_Cart"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".user.activity.P_Store"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".user.activity.P_Productpage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".user.activity.P_Order_Cart"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".user.activity.P_terms"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".user.activity.p_about"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".user.activity.P_Setting"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".user.activity.SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".user.activity.TutorialActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".user.activity.Homepage" />
        <activity android:name=".user.activity.RegisterActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".user.activity.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>